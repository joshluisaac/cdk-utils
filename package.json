{
  "name": "productapi-platform",
  "version": "1.0.0",
  "description": "AvantSystems API AWS CDK code to setup ElastiCache cluster",
  "main": "dist/index.js",
  "types": "dist",
  "scripts": {
    "build": "tsc --project .",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "prettify": "npm run prettifysrc && npm run prettifytest",
    "prettifysrc": "./node_modules/.bin/prettier   --write 'src/**/*.{ts,js,css,json}'",
    "prettifytest": "./node_modules/.bin/prettier  --write 'test/**/*.{ts,js,css,json}'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-cdk/assert": "^1.41.0",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "jest": "^26.0.1",
    "ts-jest": "^26.0.0",
    "typescript": "~3.7.2",
    "prettier-eslint-cli": "^5.0.0"
  },
  "peerDependencies": {
    "@aws-cdk/core": "^1.41.0"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.41.0",
    "@aws-cdk/aws-elasticache": "^1.41.0",
    "@aws-cdk/aws-iam": "^1.41.0",
    "@aws-cdk/aws-sns": "^1.41.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.41.0",
    "@aws-cdk/core": "^1.41.0",
    "aws-cdk": "^1.41.0",
    "aws-sdk": "^2.650.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/tests/*.spec.ts"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
